<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/backround3"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.github.mikephil.charting.charts.PieChart
        android:id="@+id/chartThang"
        android:layout_width="358dp"
        android:layout_height="261dp"
        android:layout_marginStart="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="103dp"
        android:layout_height="29dp"
        android:layout_marginStart="29dp"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="13dp"
        android:text="Tháng Này"
        android:textStyle="bold"
        android:textColor="#f00"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="353dp"
        android:layout_height="261dp"
        android:layout_marginStart="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:background="@drawable/border_main"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chartThang"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/tvTongChi"
            android:layout_width="347dp"
            android:layout_height="39dp"
            android:layout_marginStart="3dp"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="3dp"
            android:layout_marginRight="3dp"
            android:gravity="center"
            android:text="TextView"
            android:textColor="#f00"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvTienTro"
            android:layout_width="158dp"
            android:layout_height="37dp"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:text="TextView"
            android:textColor="#3F51B5"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvTongChi" />

        <TextView
            android:id="@+id/tvMuaSam"
            android:layout_width="158dp"
            android:layout_height="37dp"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="27dp"
            android:text="TextView"
            android:textColor="#3F51B5"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvTienNuoc" />

        <TextView
            android:id="@+id/tvKhac"
            android:layout_width="158dp"
            android:layout_height="37dp"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="27dp"
            android:text="TextView"
            android:textColor="#3F51B5"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/tvMuaSam"
            app:layout_constraintTop_toBottomOf="@+id/tvAnUong" />

        <TextView
            android:id="@+id/tvTienNuoc"
            android:layout_width="158dp"
            android:layout_height="37dp"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="32dp"
            android:text="TextView"
            android:textColor="#3F51B5"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvTienTro" />

        <TextView
            android:id="@+id/tvAnUong"
            android:layout_width="158dp"
            android:layout_height="37dp"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="31dp"
            android:text="TextView"
            android:textColor="#3F51B5"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/tvTienNuoc"
            app:layout_constraintTop_toBottomOf="@+id/tvTienDien" />

        <TextView
            android:id="@+id/tvTienDien"
            android:layout_width="158dp"
            android:layout_height="38dp"
            android:layout_marginStart="31dp"
            android:layout_marginLeft="31dp"
            android:layout_marginTop="21dp"
            android:text="TextView"
            android:textColor="#3F51B5"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/tvTienTro"
            app:layout_constraintTop_toBottomOf="@+id/tvTongChi" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>


